    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Ruta_2"></activity>
        <activity android:name=".Ruta_1" />
        <activity android:name=".Menu_rutas" />
        <activity android:name=".Mapa_3" />
        <activity android:name=".Mapa_2" />
        <activity android:name=".Mapa_1" />
        <activity android:name=".mapasActivity" />
        <activity android:name=".destinosActivity" />
        <activity android:name=".detalle_rutasActivity" />
        <activity android:name=".MostrarDatos" />
        <activity android:name=".RecyclerViewAdaptador" />
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
    </application>

</manifest>